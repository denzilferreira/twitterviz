<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
  http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

  <!-- prefuse GraphML Writer | Mon May 25 21:24:45 WEST 2009 -->
  <key id="keyword" for="node" attr.name="keyword" attr.type="string"/>
  <key id="id" for="node" attr.name="id" attr.type="long">
    <default>0</default>
  </key>
  <key id="screenName" for="node" attr.name="screenName" attr.type="string"/>
  <key id="relevance" for="node" attr.name="relevance" attr.type="int">
    <default>10</default>
  </key>
  <key id="friend" for="node" attr.name="friend" attr.type="boolean">
    <default>false</default>
  </key>

  <graph edgedefault="undirected">
    <!-- nodes -->
    <node id="0">
      <data key="keyword">love</data>
      <data key="id">0L</data>
      <data key="screenName">null</data>
      <data key="relevance">10</data>
      <data key="friend">false</data>
    </node>
    <node id="1">
      <data key="keyword">null</data>
      <data key="id">16393232L</data>
      <data key="screenName">1alaska</data>
      <data key="relevance">2</data>
      <data key="friend">false</data>
    </node>

    <!-- edges -->
    <edge id="0" source="0" target="1"/>
  </graph>
</graphml>
