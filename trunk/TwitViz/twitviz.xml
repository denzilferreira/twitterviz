<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
  <key id="description" for="node" attr.name="description" attr.type="string"/>
  <key id="id" for="node" attr.name="id" attr.type="long"/>
  <key id="location" for="node" attr.name="location" attr.type="string"/>
  <key id="name" for="node" attr.name="name" attr.type="string"/>
  <key id="profileImageUrl" for="node" attr.name="profileImageUrl" attr.type="string"/>
  <key id="protectedUser" for="node" attr.name="protectedUser" attr.type="boolean">
  	<default>false</default>
  </key>
  <key id="screenName" for="node" attr.name="screenName" attr.type="string"/>
  <key id="status" for="node" attr.name="status" attr.type="string"/>
  <key id="website" for="node" attr.name="website" attr.type="string"/>
  <key id="relationship" for="edge" attr.name="relationship" attr.type="string"/>
  
  <graph edgedefault="undirected">
  </graph>
</graphml>
